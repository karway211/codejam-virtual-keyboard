{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": ["error", 4, {"SwitchCase": 1}],
        "no-param-reassign": 0,
        "no-plusplus": [2, { "allowForLoopAfterthoughts": true }]
    }
}